{
    "name": "Monokai Dark Soda Modified",
    "colors": {
		"list.dropBackground": "#383b3d",
		"editor.background": "#1e1e1e",
		"editor.foreground": "#d4d4d4",
		"editor.inactiveSelectionBackground": "#3a3d41",
		"editor.selectionHighlightBackground": "#add6ff26",
		"editorIndentGuide.background": "#404040",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#d4d4d4ff",
                "background": "#1e1e1eff"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#8C8C8C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#FFEE99"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FF80F4"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#FF80F4"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FF80F4"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {}
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#F92672"
            }
        },
        {
            "name": "Keyword operator",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#80ffff"
            }
        },
        {
            "name": "Keyword operator logical",
            "scope": "keyword.operator.logical",
            "settings": {
                "foreground": "#8080c0"
            }
        },
        {
            "name": "keyword.codetag",
            "scope": "keyword.codetag",
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "italic bold underline"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#ff80ff"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#A6E22E"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#A6E22E"
            }
