            "settings": {
                "foreground": "#D3201F"
            }
        },
        {
            "name": "diff: changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#BF3904"
            }
        },
        {
            "name": "diff: inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#60cf30"
            }
